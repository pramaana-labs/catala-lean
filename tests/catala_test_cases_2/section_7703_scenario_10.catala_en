> Module Section_7703_scenario_10
> Using Sections as IRC

```catala
#[test]
declaration scope Section_7703_scenario_10:
  output person_status content IRC.IndividualSection7703MaritalStatusOutput

scope Section_7703_scenario_10:
  # Scenario: Married filing separate with residence and cost met, spouse not member during last 6 months,
  # but taxpayer not entitled to a §151 deduction for the child. Fails §7703(b)(1) due to the deduction entitlement requirement.
  # Tested law refs: §7703(b)(1)-(3)

  definition person_status equals
    let p1 equals IRC.Individual { -- id: 1 -- is_counterfactual: false } in
    let p2 equals IRC.Individual { -- id: 2 -- is_counterfactual: false } in
    let c1 equals IRC.Individual { -- id: 3 -- is_counterfactual: false } in

    let h1 equals IRC.Household { -- id: 1 -- is_counterfactual: false } in

    let marriage1 equals IRC.MarriageEvent {
      -- id: 1
      -- spouse1: p1
      -- spouse2: p2
      -- marriage_date: Date.of_year_month_day of 2012, 5, 1
      -- is_counterfactual: false
    } in

    let rp_p1 equals IRC.ResidencePeriodEvent {
      -- id: 1
      -- individual: p1
      -- household: h1
      -- start_date: Date.of_year_month_day of 2023, 1, 1
      -- end_date: Date.of_year_month_day of 2023, 12, 31
      -- is_member_of_household: true
      -- is_principal_place_of_abode: true
      -- is_counterfactual: false
    } in
    let rp_c1 equals IRC.ResidencePeriodEvent {
      -- id: 2
      -- individual: c1
      -- household: h1
      -- start_date: Date.of_year_month_day of 2023, 1, 1
      -- end_date: Date.of_year_month_day of 2023, 12, 31
      -- is_member_of_household: true
      -- is_principal_place_of_abode: true
      -- is_counterfactual: false
    } in
    let rp_p2 equals IRC.ResidencePeriodEvent {
      -- id: 3
      -- individual: p2
      -- household: h1
      -- start_date: Date.of_year_month_day of 2023, 1, 1
      -- end_date: Date.of_year_month_day of 2023, 5, 31
      -- is_member_of_household: true
      -- is_principal_place_of_abode: false
      -- is_counterfactual: false
    } in

    let hm_p1 equals IRC.HouseholdMaintenanceEvent {
      -- id: 1
      -- individual: p1
      -- household: h1
      -- start_date: Date.of_year_month_day of 2023, 1, 1
      -- end_date: Date.of_year_month_day of 2023, 12, 31
      -- cost_furnished_percentage: 0.65
      -- is_counterfactual: false
    } in

    let parent1 equals IRC.ParenthoodEvent {
      -- id: 1
      -- parent: p1
      -- child: c1
      -- start_date: Date.of_year_month_day of 2010, 1, 1
      -- parent_type: IRC.ParentType.Biological
      -- is_counterfactual: false
    } in

    # Married Filing Separate return for tax year 2023
    let mfs_variant equals IRC.MarriedFilingSeparateVariant {
      -- taxpayer: p1
      -- spouse: p2
      -- itemization_election: false
      -- spouse_itemization_election: false
      -- is_estate_or_trust: false
      -- is_common_trust_fund: false
      -- is_partnership: false
    } in
    let individual_tax_return equals IRC.IndividualTaxReturn {
      -- id: 1
      -- tax_year: 2023
      -- details: IRC.FilingStatusVariant.MarriedFilingSeparate content mfs_variant
    } in

    # Precomputed §152 qualifying child result for c1 relative to p1
    let qc_c1 equals IRC.IndividualSection152QualifyingChildOutput {
      -- individual: c1
      -- taxpayer: p1
      -- is_qualifying_child: true
      -- bears_relationship_152_c_1_A: true
      -- has_same_principal_place_of_abode_152_c_1_B: true
      -- meets_age_requirements_152_c_1_C: true
      -- has_not_filed_joint_return_152_c_1_E: true
    } in

    # Call §7703 marital status scope with assumption:
    # individuals_entitled_to_exemptions_under_151 = [] (no §151 entitlement for the child)
    (output of IRC.IndividualSection7703MaritalStatus with {
      -- individual: p1
      -- tax_year: 2023
      -- marriage_events: [ marriage1 ]
      -- divorce_or_legal_separation_events: []
      -- death_events: []
      -- individual_tax_return: individual_tax_return
      -- residence_period_events: [ rp_p1 ] ++ [ rp_c1 ] ++ [ rp_p2 ]
      -- household_maintenance_events: [ hm_p1 ]
      -- qualifying_children: [ qc_c1 ]
      -- individuals_entitled_to_exemptions_under_151: []
    }).main_output

  # Assertions per scenario and §7703(b):
  # - Residence and cost are met, and spouse not a member during last 6 months
  # - §151 entitlement is not met, so overall §7703(b) conditions are not satisfied

  # Components (b)(2) and (b)(3) should be true under the scenario
  assertion (person_status.furnishes_over_one_half_cost_7703_b_2 = true)
  assertion (person_status.spouse_not_member_last_6_months_7703_b_3 = true)

  # Overall §7703(b) conditions are not met (composite of (b)(1)-(3) fails)
  assertion ((person_status.maintains_household_7703_b_1
             and person_status.furnishes_over_one_half_cost_7703_b_2
             and person_status.spouse_not_member_last_6_months_7703_b_3) = false)

  # Consequently, taxpayer is not treated as "not married" under §7703(b)
  assertion (person_status.considered_not_married_7703_b = false)
