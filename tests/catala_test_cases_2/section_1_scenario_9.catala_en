> Module Section_1_scenario_9
> Using Sections as IRC

```catala
#[test]
declaration scope Section_1_scenario_9:
  output tax_result content IRC.IndividualTaxReturnSection1TaxOutput

scope Section_1_scenario_9:
  # Scenario: Head of household with taxable income of $100,000
  # Tested law refs: §1(b) (Heads of households)
  # Bracket per law text: "Over $76,400 but not over $127,500" → tax = $17,544 + 31% of excess over $76,400
  # Computation per comments: $17,544 + 31% * ($100,000 − $76,400 = $23,600) = $24,860

  definition tax_result equals
    # Individuals
    let hho3 equals IRC.Individual { -- id: 1 -- is_counterfactual: false } in
    let c3 equals IRC.Individual { -- id: 2 -- is_counterfactual: false } in

    # Head of Household return for 2023 with qualifying person c3
    let hoh_variant equals IRC.HeadOfHouseholdVariant {
      -- taxpayer: hho3
      -- qualifying_person: c3
      -- itemization_election: false
      -- is_estate_or_trust: false
      -- is_common_trust_fund: false
      -- is_partnership: false
    } in
    let individual_tax_return equals IRC.IndividualTaxReturn {
      -- id: 1
      -- tax_year: 2023
      -- details: IRC.FilingStatusVariant.HeadOfHousehold content hoh_variant
    } in

    # Temporary marital status output for §7703 to satisfy dependency of §1 scope
    # Set to "not married for tax purposes" and with deemed-unmarried supportive flags
    let taxpayer_marital_status equals IRC.IndividualSection7703MaritalStatusOutput {
      -- individual: hho3
      -- tax_year: 2023
      -- determination_date_7703_a_1: Date.of_year_month_day of 2023, 12, 31
      -- is_married_7703_a_1: false
      -- is_legally_separated_7703_a_2: false
      -- maintains_household_7703_b_1: true
      -- furnishes_over_one_half_cost_7703_b_2: true
      -- spouse_not_member_last_6_months_7703_b_3: true
      -- considered_not_married_7703_b: true
      -- is_married_for_tax_purposes_7703: false
    } in

    # Call §1 tax computation; provide bracket-specific context values per §1(b)
    (output of IRC.IndividualTaxReturnSection1Tax with {
      -- individual_tax_return: individual_tax_return
      -- taxable_income: $100000
      -- taxpayer_marital_status: taxpayer_marital_status
      -- is_surviving_spouse: false
      -- is_head_of_household: true

      # Context fields: set only the §1(b) bracket value; others zero
      -- tax_1_a_1: $0
      -- tax_1_a_2: $0
      -- tax_1_b: $24860
      -- tax_1_c: $0
      -- tax_1_d: $0
      -- tax: $24860
    }).main_output

  # Assertions - ONLY reference the computed scope output (no additional computations)

  # section_1_tax_amount = $24,860
  # Correlates to §1(b) third bracket computation
  assertion (tax_result.tax = $24860)

  # section_1_bracket_applied = "Over $76,400 but not over $127,500"
  # We assert that the §1(b) field carries the tax amount (i.e., §1(b) bracket applied)
  assertion (tax_result.tax_1_b = $24860)

  # section_1_status_applied = "HeadOfHousehold under §1(b)"
  # We assert the filing status variant in the output is HeadOfHousehold (the §1(b) status)
  assertion (
    tax_result.individual_tax_return.details
      with pattern IRC.FilingStatusVariant.HeadOfHousehold
  )
