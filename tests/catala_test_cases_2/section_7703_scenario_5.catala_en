> Module Section_7703_scenario_5
> Using Sections as IRC

```catala
#[test]
declaration scope Section_7703_scenario_5:
  output person_status content IRC.IndividualSection7703MaritalStatusOutput

scope Section_7703_scenario_5:
  # Scenario: §7703(b) deemed-unmarried test for a married individual filing separately
  # Tested law refs: §7703(a)(1), §7703(b)(1), §7703(b)(2), §7703(b)(3)
  # Assumption: Taxpayer is entitled to a §151 deduction for child c1 (modeled by passing c1
  # in individuals_entitled_to_exemptions_under_151 and constructing a QC output for c1)

  definition person_status equals
    let p1 equals IRC.Individual { -- id: 1 -- is_counterfactual: false } in
    let p2 equals IRC.Individual { -- id: 2 -- is_counterfactual: false } in
    let c1 equals IRC.Individual { -- id: 3 -- is_counterfactual: false } in

    let h1 equals IRC.Household { -- id: 1 -- is_counterfactual: false } in

    let marriage1 equals IRC.MarriageEvent {
      -- id: 1
      -- spouse1: p1
      -- spouse2: p2
      -- marriage_date: Date.of_year_month_day of 2016, 1, 1
      -- is_counterfactual: false
    } in

    let res_p1 equals IRC.ResidencePeriodEvent {
      -- id: 1
      -- individual: p1
      -- household: h1
      -- start_date: Date.of_year_month_day of 2023, 1, 1
      -- end_date: Date.of_year_month_day of 2023, 12, 31
      -- is_member_of_household: true
      -- is_principal_place_of_abode: true
      -- is_counterfactual: false
    } in
    let res_c1 equals IRC.ResidencePeriodEvent {
      -- id: 2
      -- individual: c1
      -- household: h1
      -- start_date: Date.of_year_month_day of 2023, 1, 1
      -- end_date: Date.of_year_month_day of 2023, 9, 30
      -- is_member_of_household: true
      -- is_principal_place_of_abode: true
      -- is_counterfactual: false
    } in
    let res_p2 equals IRC.ResidencePeriodEvent {
      -- id: 3
      -- individual: p2
      -- household: h1
      -- start_date: Date.of_year_month_day of 2023, 1, 1
      -- end_date: Date.of_year_month_day of 2023, 5, 31
      -- is_member_of_household: true
      -- is_principal_place_of_abode: false
      -- is_counterfactual: false
    } in

    let hmc_p1 equals IRC.HouseholdMaintenanceEvent {
      -- id: 1
      -- individual: p1
      -- household: h1
      -- start_date: Date.of_year_month_day of 2023, 1, 1
      -- end_date: Date.of_year_month_day of 2023, 12, 31
      -- cost_furnished_percentage: 0.6
      -- is_counterfactual: false
    } in

    let parent_p1_c1 equals IRC.ParenthoodEvent {
      -- id: 1
      -- parent: p1
      -- child: c1
      -- start_date: Date.of_year_month_day of 2010, 1, 1
      -- parent_type: IRC.ParentType.Biological
      -- is_counterfactual: false
    } in

    # Married filing separate individual income tax return for tax year 2023
    let mfs_variant equals IRC.MarriedFilingSeparateVariant {
      -- taxpayer: p1
      -- spouse: p2
      -- itemization_election: false
      -- spouse_itemization_election: false
      -- is_estate_or_trust: false
      -- is_common_trust_fund: false
      -- is_partnership: false
    } in
    let individual_tax_return equals IRC.IndividualTaxReturn {
      -- id: 1
      -- tax_year: 2023
      -- details: IRC.FilingStatusVariant.MarriedFilingSeparate content mfs_variant
    } in

    # Temporary Qualifying Child output for c1 relative to p1 (assumed true on all §152(c) tests)
    let qc_c1 equals IRC.IndividualSection152QualifyingChildOutput {
      -- individual: c1
      -- taxpayer: p1
      -- is_qualifying_child: true
      -- bears_relationship_152_c_1_A: true
      -- has_same_principal_place_of_abode_152_c_1_B: true
      -- meets_age_requirements_152_c_1_C: true
      -- has_not_filed_joint_return_152_c_1_E: true
    } in

    # Call §7703 marital status scope; pass §151 entitlement assumption via the list of individuals
    (output of IRC.IndividualSection7703MaritalStatus with {
      -- individual: p1
      -- tax_year: 2023
      -- marriage_events: [ marriage1 ]
      -- divorce_or_legal_separation_events: []
      -- death_events: []
      -- individual_tax_return: individual_tax_return
      -- residence_period_events: [ res_p1 ] ++ [ res_c1 ] ++ [ res_p2 ]
      -- household_maintenance_events: [ hmc_p1 ]
      -- qualifying_children: [ qc_c1 ]
      -- individuals_entitled_to_exemptions_under_151: [ c1 ]
    }).main_output

  # Assertions - Only check outputs of the computed scope
  # §7703(a)(1): determine if married as of close of tax year
  assertion (person_status.is_married_7703_a_1 = true)

  # §7703(b)(1): maintains as home a household that is principal place of abode of the child > half year
  assertion (person_status.maintains_household_7703_b_1 = true)

  # §7703(b)(2): furnishes > half the cost of maintaining such household
  assertion (person_status.furnishes_over_one_half_cost_7703_b_2 = true)

  # §7703(b)(3): during last 6 months, spouse not a member of the household
  assertion (person_status.spouse_not_member_last_6_months_7703_b_3 = true)

  # Conclusion of §7703(b): individual shall not be considered as married
  assertion (person_status.considered_not_married_7703_b = true)
