  assertion (result.standard_deduction_63_c_1 = $0)

  # Corroborates the legal reason: filer flagged as common trust fund ⇒ not eligible for standard deduction under §63(c)(6)(D)
  assertion (
    match result.individual_tax_return.details with pattern
    -- IRC.FilingStatusVariant.Single content variant :
      variant.is_common_trust_fund = true
    -- anything : false
  )

  # Confirms ineligibility flag used by §63(c)(6) logic is set to false (no standard deduction eligibility)
  assertion (result.standard_deduction_eligible_63_c_6 = false)
