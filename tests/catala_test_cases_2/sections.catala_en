```catala
scope IndividualSection7703MaritalStatus:
  # LAW: ยง7703(b)(2) - Individual furnishes over one-half of the cost
  # Such individual furnishes over one-half of the cost of maintaining such household during the taxable year
  # The household must be the individual's principal place of abode
  label section_7703_b_2_households_maintained
  definition households_maintained_by_individual equals
    let year_end equals Date.of_year_month_day of tax_year, 12, 31 in
    let year_start equals Date.of_year_month_day of tax_year, 1, 1 in
    let year_midpoint equals Date.of_year_month_day of tax_year, 6, 30 in
    # Find households where:
    # 1. Individual has principal place of abode for more than half the year
    # 2. Individual furnishes over one-half of the cost
    map each individual_residence_event among residence_period_events
      such that individual_residence_event.individual = individual
      and individual_residence_event.is_principal_place_of_abode
      # Principal place of abode must be for more than half the year
      and period_spans_more_than_half_year of individual_residence_event.start_date, individual_residence_event.end_date, tax_year
      and individual_residence_event.is_member_of_household
      # Individual must furnish over one-half of the cost
      and exists maintenance_event among household_maintenance_events
        such that maintenance_event.individual = individual
        and maintenance_event.household = individual_residence_event.household
        and maintenance_event.cost_furnished_percentage > 0.5
        # Maintenance period must overlap with tax year
        and maintenance_event.start_date <= year_end
        and maintenance_event.end_date >= year_start
        # Maintenance period should overlap with residence period
        and maintenance_event.start_date <= individual_residence_event.end_date
        and maintenance_event.end_date >= individual_residence_event.start_date
    to
      individual_residence_event.household

  # LAW: ยง7703(b)(2) - Boolean version for output
  # Furnishes over one-half of the cost of maintaining such household
  label section_7703_b_2_boolean
  definition furnishes_over_one_half_of_household_cost equals
    number of households_maintained_by_individual > 0
