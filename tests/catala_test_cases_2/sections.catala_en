    let last_6_months_start equals Date.of_year_month_day of tax_year, 7, 1 in
    let last_6_months_end equals year_end in
    let spouse_from_tax_return equals section_7703_get_spouse_from_tax_return of individual, individual_tax_return in
    match spouse_from_tax_return with pattern
    -- Present content spouse :
      # Condition (3): Check if there exists a household where taxpayer is a member and spouse is NOT a member during last 6 months
      # Loop through residence events to find households where taxpayer is a member
      exists taxpayer_residence_event among residence_period_events
        such that taxpayer_residence_event.individual = individual
        and taxpayer_residence_event.is_member_of_household
        # Check if this residence overlaps with the last 6 months
        and taxpayer_residence_event.start_date <= last_6_months_end
        and taxpayer_residence_event.end_date >= last_6_months_start
        # Check if spouse is NOT a member of this household during the last 6 months
        and not (section_7703_is_spouse_member_of_household_last_6_months of spouse, taxpayer_residence_event.household, residence_period_events, last_6_months_start, last_6_months_end)
    -- Absent :
      # If no spouse exists, condition (3) is automatically satisfied
      true
```

