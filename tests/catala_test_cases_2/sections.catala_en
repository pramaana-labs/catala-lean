```catala-metadata
scope IndividualTaxReturnSection2aSurvivingSpouse:
  definition
    spouse_death_events_in_window
    of spouse_arg, death_events_arg, tax_year_arg equals
    match spouse_arg with pattern
    -- Present content s :
      list of death_event among death_events_arg
        such that death_event.decedent = s
        and Date.get_year of death_event.death_date >= tax_year_arg - 2
        and Date.get_year of death_event.death_date < tax_year_arg
    -- Absent : list of death_event among death_events_arg such that false

  definition
    spouse_death_year
    of spouse_death_events_in_window_arg equals
    if number of spouse_death_events_in_window_arg > 0 then
      Present content (maximum of (map each death_event among spouse_death_events_in_window_arg to Date.get_year of death_event.death_date))
    else Absent

  definition
    most_recent_spouse_death_date
    of spouse_death_events_in_window_arg, tax_year_arg equals
    if number of spouse_death_events_in_window_arg > 0 then
      maximum of (map each death_event among spouse_death_events_in_window_arg to death_event.death_date)
    else Date.of_year_month_day of tax_year_arg, 1, 1

  definition main_output equals
    IndividualTaxReturnSection2aSurvivingSpouseOutput {
      -- individual_tax_return: individual_tax_return
      -- spouse_died_within_two_years_2_a_1_A: spouse_died_within_two_years_2_a_1_A
      -- maintains_household_qualifying_dependent_principal_abode_section151_2_a_1_B: maintains_household_qualifying_dependent_principal_abode_section151_2_a_1_B
      -- has_remarried_2_a_2_A: has_remarried_2_a_2_A
      -- joint_return_could_have_been_made_2_a_2_B: joint_return_could_have_been_made_2_a_2_B
      -- is_surviving_spouse_2_a: is_surviving_spouse_2_a
