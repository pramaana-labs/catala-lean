> Module Section_3306_scenario_16
> Using Sections as IRC

```catala
#[test]
declaration scope Section_3306_scenario_16:
  output computation content IRC.IRCSimplified

scope Section_3306_scenario_16:
  # Scenario: §3306(b)(11) exclusion for agricultural noncash remuneration; such noncash remuneration
  # does not count toward §3306(a)(2)(A) $20,000 agricultural wages threshold, and no 10-day/5-individual
  # threshold is met. Taxable FUTA wages after §3306(b) exclusions are $0.
  # Tested law refs: §3306(b)(11), §3306(a)(2)(A)

  definition computation equals
    let ag1 equals IRC.Individual { -- id: 1 -- is_counterfactual: false } in

    let org16 equals IRC.Organization {
      -- id: 1
      -- organization_type: IRC.OrganizationType.Business
      -- is_counterfactual: false
    } in

    # Employment relationship: Agricultural labor in April 2024
    let emp_rel_1 equals IRC.EmploymentRelationshipEvent {
      -- id: 1
      -- employer: org16
      -- employee: ag1
      -- start_date: Date.of_year_month_day of 2024, 4, 1
      -- end_date: Date.of_year_month_day of 2024, 4, 30
      -- employment_category: IRC.EmploymentCategory.AgriculturalLabor
      -- service_location: IRC.ServiceLocation.WithinUnitedStates
      -- is_american_employer: false
      -- employee_is_us_citizen: true
      -- is_counterfactual: false
    } in

    # Wage payment: $25,000 in NONCASH remuneration for agricultural labor -> excluded by §3306(b)(11)
    let wage1 equals IRC.WagePaymentEvent {
      -- id: 1
      -- employer: org16
      -- employee: ag1
      -- payment_date: Date.of_year_month_day of 2024, 4, 15
      -- amount: $25000
      -- payment_medium: IRC.PaymentMedium.NonCash
      -- payment_reason: IRC.PaymentReason.AgriculturalLaborNonCash
      -- is_under_plan_or_system: false
      -- is_for_employee_generally: false
      -- is_for_class_of_employees: false
      -- is_for_dependents: false
      -- is_not_in_course_of_trade_or_business: false
      -- would_have_been_paid_without_termination: true
      -- is_paid_to_survivor_or_estate: false
      -- is_counterfactual: false
    } in

    # Minimal individual income tax return (not used for FUTA, but required by IRCSimplified)
    let single_variant equals IRC.SingleVariant {
      -- taxpayer: ag1
      -- itemization_election: false
      -- is_estate_or_trust: false
      -- is_common_trust_fund: false
      -- is_partnership: false
    } in
    let individual_tax_return equals IRC.IndividualTaxReturn {
      -- id: 1
      -- tax_year: 2024
      -- details: IRC.FilingStatusVariant.Single content single_variant
    } in

    # Employer FUTA return (Agricultural employer context)
    let eutr1_details equals IRC.EmployerAgriculturalLaborVariant { -- employer: org16 } in
    let eutr1 equals IRC.EmployerUnemploymentExciseTaxReturn {
      -- id: 1
      -- tax_year: 2024
      -- details: IRC.EmployerVariant.AgriculturalEmployer content eutr1_details
    } in

    output of IRC.IRCSimplified with {
      -- individual_tax_return: individual_tax_return
      -- individuals: [ ag1 ]
      -- organizations: [ org16 ]
      -- birth_events: []
      -- blindness_status_events: []
      -- death_events: []
      -- nonresident_alien_status_period_events: []
      -- marriage_events: []
      -- divorce_or_legal_separation_events: []
      -- residence_period_events: []
      -- household_maintenance_events: []
      -- parenthood_events: []
      -- family_relationship_events: []
      -- tax_return_events: []
      -- income_events: []
      -- employment_relationship_events: [ emp_rel_1 ]
      -- wage_payment_events: [ wage1 ]
      -- student_enrollment_events: []
      -- hospital_patient_events: []
      -- immigration_admission_events: []
      -- employment_termination_events: []
      -- employer_unemployment_excise_tax_return: eutr1
      -- itemized_deductions: $0
      -- adjusted_gross_income: $0
    }

  # Assertions:

  # §3306(b)(11): Agricultural labor remuneration paid in a medium other than cash is excluded from wages.
  # Expect the sole wage event (id=1) to be flagged excluded_by_agricultural_noncash and contribute $0 taxable before cap.
  assertion (
    exists r among computation.wage_payment_wages_results
      such that r.wage_payment_event.id = 1
        and r.is_excluded_by_agricultural_noncash = true
        and r.taxable_amount_before_7000_cap = $0
  )

  # Correlate excluded amount to the paid noncash remuneration ($25,000) under §3306(b)(11)
  assertion (
    exists r among computation.wage_payment_wages_results
      such that r.wage_payment_event.id = 1
        and r.is_excluded_by_agricultural_noncash = true
        and r.wage_payment_event.amount = $25000
  )

  # §3306(a)(2)(A): Agricultural employer wages threshold requires $20,000 in CASH wages.
  # With only noncash agricultural remuneration, threshold is not met; employer should not qualify as agricultural employer.
  assertion (
    exists s among computation.organization_employer_statuses
      such that s.organization.id = 1
        and s.is_agricultural_employer = false
        and s.is_employer = false
  )

  # §3306(b): Total taxable FUTA wages after exclusions should be $0 (all remuneration was excluded by (b)(11)).
  assertion (computation.employer_unemployment_excise_tax_result.total_taxable_wages = $0)
```