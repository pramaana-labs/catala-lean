  # Assertions - ONLY reference the scope output
  # §3306(c)(6): service in the employ of the United States Government is excluded
  assertion (employment_exclusions.1.is_excluded_federal_government = true)

  # §3306(c)(7): service in the employ of a State (or political subdivision) is excluded
  assertion (employment_exclusions.2.is_excluded_state_government = true)

  # §3306(c)(11): service in the employ of a foreign government is excluded
  assertion (employment_exclusions.3.is_excluded_foreign_government = true)

  # §3306(c)(16): service in the employ of an international organization is excluded
  assertion (employment_exclusions.4.is_excluded_international_organization = true)

  # §3306(c)(21): service by a person committed to a penal institution is excluded
  assertion (employment_exclusions.5.is_excluded_penal_institution = true)
